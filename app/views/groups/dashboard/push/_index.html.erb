<div class="row">
  <div class="grid-element col-sm-12 col-md-6 blue-dam-1">
    <div class="post-block">
      <div class="title-white text-center">
        <i class="fa fa-trophy" aria-hidden="true"></i><span>Award Message</span>
      </div>
      <div id="award_message">
        <%= render 'groups/dashboard/push/push_form', push: :awards %>
      </div>
    </div>
  </div>
  <div class="grid-element col-sm-12 col-md-6 blue-dam-2">
    <div class="post-block">
      <div class="title-white text-center">
        <i class="fa fa-thumbs-o-up" aria-hidden="true"></i><span>Incentive Message</span>
      </div>
      <div id="incentive_message">
        <%= render 'groups/dashboard/push/push_form', push: :incentive %>
      </div>
    </div>
  </div>
</div>
